#include <stdio.h>

int main() {
    // Initialize the array
    int arr[] = {1, 2, 3, 4, 5};
    
    // Update the value at index 2 (third element)
    arr[2] = 100;  // Changing value 3 to 100
    
    // Output the updated array
    for (int i = 0; i < 5; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
